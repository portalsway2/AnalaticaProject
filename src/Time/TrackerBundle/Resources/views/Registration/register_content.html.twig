{% trans_default_domain 'FOSUserBundle' %}

{% block body %}

    {% image "bundles/timetracker/image/download.jpg" %}
    <img src="{{ asset_url }}"/>
    {% endimage %}

    <div class="trois">
        <form action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST"
              class="fos_user_registration_register">
            {{ form_widget(form) }}

            <div>
                <input type="submit" value="{{ 'registration'|trans }}"/>
            </div>
        </form>
    </div>
{% endblock %}