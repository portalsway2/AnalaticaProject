        padding: 10px;
        border-style: solid;
        border-color: #000000;
    }

    .content {
        min-height: 100%;
        position: relative;
        overflow: auto;
        z-index: 0;
    }

    .top_block {
        width: 100%;
        display: block;
    }

    .block_1 {
