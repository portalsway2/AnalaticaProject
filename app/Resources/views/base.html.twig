        text-shadow: 3px 1px 0px #fff, 2px 2px 0px rgba(0, 0, 0, 0.1), 3px 3px 0px #fff, 4px 4px 10px rgba(0, 0, 0, 0.1), 5px 5px 14px #fff;
    }

    .left {
        float: left;
    }

    form {
        width: 450px;
        margin: 0px auto;
        text-align: left;
    }

    p {
        padding: 26px 0px;
        border-top: 1px dotted #c7c7c7;
    }

    li {
        padding: 26px 0px;
        border-top: 1px dotted #c7c7c7;
        min-height: 27px;
    }

    label {
        float: left;
        width: 200px;
    }

    </style>

